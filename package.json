{
  "name": "alphalabs",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --handle-input --prefix-colors \"blue,green\" --prefix \"[{name}]\" --names \"frontend,backend\" \"bun run dev:frontend\" \"bun run dev:backend\"",
    "dev:frontend": "bun run --filter @alphalabs/frontend dev",
    "dev:backend": "cd backend; python app.py",
    "build": "bun run --filter @alphalabs/frontend build",
    "start": "bun run --filter @alphalabs/frontend start",
    "install:all": "bun install"
  },
  "devDependencies": {
    "bun": "^1.0.0",
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  }
}
